
# this is for E-DIO24 measurement computing.



file "$(MEASCOMP)/measCompApp/Db/measCompLongIn.template"
{
pattern
{    P,       R,   MASK,   PORT,    ADDR}
{   MEXDAQEDIO24:,  Li,   0xFFFFFF,  EDIO24_1,     0}

}

