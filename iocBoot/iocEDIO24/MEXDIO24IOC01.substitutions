{   MEXDAQEDIO24:,  Bd11,  0x000400    0,  EDIO24_1,     0}
{   MEXDAQEDIO24:,  Bd12,  0x000800    0,  EDIO24_1,     0}
{   MEXDAQEDIO24:,  Bd13,  0x001000    0,  EDIO24_1,     0}
